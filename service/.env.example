# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# AI Service API Keys
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Per-Task LLM Configuration (format: provider:model)
# Available providers: gemini, openai
# Gemini models: gemini-2.5-pro
# OpenAI models: gpt-5, gpt-4o, gpt-4-turbo
LLM_FEATURES=gemini:gemini-2.5-pro
LLM_INSIGHTS=openai:gpt-5
LLM_PATCH=openai:gpt-5
LLM_BRIEF=gemini:gemini-2.5-pro
LLM_ANALYZE=openai:gpt-5
LLM_EDIT=openai:gpt-5

# Debug settings
DEBUG_LLM=true
